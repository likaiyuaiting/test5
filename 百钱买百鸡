// 百钱买百鸡.cpp : 定义控制台应用程序的入口点。
//

#include "stdafx.h"
#include<stdio.h>
#include<cstdlib>
int main(void)
{
	int i, j, k;
	for (i = 0; i <= 20;i++)
	for (j = 0; j < 33;j++)
	for (k = 0; k < 300; k++)
	{
		if (i + j + k == 100 && 15 * i + 9 * j + k == 300)
			printf("公鸡买%d只，母鸡买%d只，小鸡买%d只。\n", i, j, k);
	}
	system("pause");
	return 0;
}

